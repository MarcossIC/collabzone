{
  "name": "collab-zone",
  "version": "0.0.1",
  "description": "Applicacion de oficinas online",
  "author": "Marcos",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist; exit 0",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"data/**/*.ts\"  --ignore-path .prettierignore",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:yaml": "node data/scripts/runyamllint.js",
    "commitlint:last": "npx commitlint --from HEAD~1 --to HEAD --verbose || $1",
    "test": "cross-env NODE_ENV=automated_tests jest",
    "test:watch": "cross-env NODE_ENV=automated_tests jest --watch",
    "test:cov": "cross-env NODE_ENV=automated_tests jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typos": "node data/scripts/checktypos.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/configuration/orm.configuration.ts",
    "schema:drop": "cross-env NODE_ENV=development npm run typeorm -- schema:drop",
    "schema:sync": "cross-env NODE_ENV=development npm run typeorm -- schema:sync",
    "migration:create": "cross-var ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./data/migrations/$npm_config_name",
    "migration:generate": "cross-var npm run typeorm migration:generate ./data/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "fixtures": "cross-env NODE_ENV=development fixtures load ./data/fixture --sync --debug --dataSource=./src/configuration/orm.configuration.ts --require=ts-node/register --require=tsconfig-paths/register",
    "prepare": "node setup-husky.js",
    "postinstall": "node setup-husky.js"
  },
  "dependencies": {
    "@as-integrations/fastify": "^1.3.0",
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/csrf-protection": "^6.4.1",
    "@keyv/redis": "3.0.1",
    "@mercuriusjs/gateway": "^2.1.0",
    "@nestjs/cache-manager": "2.3.0",
    "@nestjs/common": "10.2.10",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/event-emitter": "2.1.1",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "2.0.5",
    "@nestjs/mercurius": "^12.0.11",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-fastify": "^10.2.10",
    "@nestjs/throttler": "6.2.1",
    "@nestjs/typeorm": "10.0.2",
    "bcrypt": "5.1.1",
    "better-sqlite3": "8.7.0",
    "cache-manager": "5.7.6",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.0",
    "fastify": "^4.24.3",
    "graphql": "16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload-minimal": "1.5.5",
    "ioredis": "5.4.1",
    "joi": "17.13.3",
    "jwks-rsa": "3.1.0",
    "keyv": "5.1.3",
    "mercurius": "^13.3.1",
    "mercurius-cache": "^6.0.0",
    "mercurius-upload": "^7.0.0",
    "mqemitter-redis": "^6.0.0",
    "mysql2": "2.3.3",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pino": "9.5.0",
    "pino-http": "10.3.0",
    "pino-pretty": "11.3.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sqlite3": "5.1.4",
    "typeorm": "0.3.11",
    "typeorm-naming-strategies": "4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@commitlint/types": "19.5.0",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@faker-js/faker": "9.2.0",
    "@nestjs/cli": "10.4.7",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.2.10",
    "@swc/cli": "0.5.0",
    "@swc/core": "1.7.42",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/bcrypt": "5.0.2",
    "@types/cache-manager": "4.0.6",
    "@types/express": "5.0.0",
    "@types/fs-extra": "11.0.4",
    "@types/graphql-upload": "16.0.7",
    "@types/jest": "29.5.14",
    "@types/node": "22.8.6",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "cross-env": "7.0.3",
    "cross-var": "1.1.0",
    "dotenv": "16.4.5",
    "env-cmd": "10.1.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.9.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-unicorn": "56.0.0",
    "fs-extra": "11.2.0",
    "globals": "15.12.0",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "15.2.10",
    "nyc": "17.1.0",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.19.2",
    "typeorm-fixtures-cli": "4.0.0",
    "typescript": "5.6.3",
    "unplugin-swc": "1.5.1",
    "vite": "5.4.10"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "moduleNameMapper": {
      "@modules/(.*)$": "<rootDir>/src/modules/$1",
      "@common/(.*)$": "<rootDir>/src/common/$1",
      "^@root/(.*)$": "<rootDir>/$1",
      "^@data/(.*)$": "<rootDir>/data/$1",
      "^@/(.*)$": "<rootDir>/src/$1",
      "@configuration/(.*)$": "<rootDir>/src/configuration/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "/coverage",
    "testEnvironment": "node",
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}